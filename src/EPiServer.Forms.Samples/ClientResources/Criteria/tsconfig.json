{
    "compilerOptions": {
        "baseUrl": ".",
        "rootDir": ".",
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "suppressImplicitAnyIndexErrors": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "jsx": "react"
    },
    "exclude": [
        "**/dist"
    ]
}